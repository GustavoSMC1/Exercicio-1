def soma_divisores(num):
    s1, s2 = 0, 0
    for i in range(1, num + 1):
        if num % i == 0:  
            if i % 2 == 0:
                s2 += i
            else:
                s1 += i
    return s1, s2


n = int(input("Digite um número: "))
soma_impares, soma_pares = soma_divisores(n)
print("Soma dos divisores ímpares (S1):", soma_impares)
print("Soma dos divisores pares (S2):", soma_pares)
